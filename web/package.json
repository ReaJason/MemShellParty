    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.3.4",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.0.8",
    "typescript": "^5.7.3",
    "vite": "^6.2.0",
    "vite-bundle-visualizer": "^1.2.1"
